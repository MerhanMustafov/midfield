{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "singleQuote": true,
  "semi": true,
  "trailingComma": "all",
  "printWidth": 100,
  "tabWidth": 2,
  "importOrder": [
    "module-alias/register",
    "@configs/dotenv.config",
    "@configs/axios.config",
    "<THIRD_PARTY_MODULES>",
    "^@configs/(.*)$",
    "^@constants/(.*)$",
    "^@middlewares/(.*)$",
    "^@routes/(.*)$",
    "^@controllers/(.*)$",
    "^@utils/(.*)$",
    "^@data/(.*)$",
    "^[./]"
  ]
}
